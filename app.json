      "package": "com.mpaice.radiosweetlordbrasil"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6af553da-8d31-4677-ba66-e5f107f301c0"
      }
